	"version": "0.2.0",
	"configurations": [
		{
		"name": "cl.exe build and debug active file",
		"type": "cppvsdbg",
		"request": "launch",
		"program": "${workspaceFolder}\\${fileBasenameNoExtension}.exe",
		"args": ["--show-lex", "--show-parse", "step", "--gvCPX", "--i", "examples\\07-relative-move.rc"],
		"stopAtEntry": false,
		"cwd": "${workspaceFolder}",
		"environment": [],
